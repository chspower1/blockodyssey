{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@api/*": [
                "src/api/*"
            ],
            "@type/*": [
                "src/type/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@store/*": [
                "src/store/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@constant/*": [
                "src/constant/*"
            ]
        }
    }
}